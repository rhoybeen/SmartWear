<vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="@dimen/tic_compound_button_drawable_size_small"
    android:height="@dimen/tic_compound_button_drawable_size_small"
    android:viewportHeight="104.0"
    android:viewportWidth="104.0">
    <group
        android:name="container"
        android:pivotX="52"
        android:pivotY="52">
        <path
            android:name="bgPath"
            android:pathData="@string/tic.svg.btn.two_state.bg"
            android:fillColor="?android:colorControlActivated" />

        <group android:name="fgOn"
               android:pivotX="52.0"
               android:pivotY="52.0">
            <path
                android:fillColor="#FFFFFF"
                android:pathData="@string/tic.svg.btn.radio.on" />
        </group>
    </group>
</vector>
